    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.11.1",
    "eslint-plugin-vue": "^8.2.0",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.15",
    "typescript": "~5.6.2",
    "vite": "^2.7.7",
